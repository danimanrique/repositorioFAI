                    <dc-qualifier>other</dc-qualifier>
                    <repeatable>true</repeatable>
                    <label>Director/Codirector</label>
                    <input-type>twobox</input-type>
                    <hint>Indique en este campo el nombre del director y/o codirector</hint>
                    <required>Campo obligatorio</required>
                </field>

                <field>
                    <dc-schema>dc</dc-schema>
                    <dc-element>date</dc-element>
                    <dc-qualifier>issued</dc-qualifier>
                    <repeatable>false</repeatable>
                    <label>Fecha</label>
                    <input-type>date</input-type>
                    <hint>Fecha de defensa (dictamen)</hint> 
                    <required>Campo obligatorio</required>
                </field>

                <field>
                    <dc-schema>dc</dc-schema>
                    <dc-element>identifier</dc-element>
                    <dc-qualifier></dc-qualifier>
                    <repeatable>false</repeatable>
                    <label>Tipo</label>
                    <input-type value-pairs-name="tipos">dropdown</input-type>
                    <hint>Naturaleza o genero del contenido de la tesis</hint>
                    <required>Campo Obligatorio</required>
                </field>

                <field>
                    <dc-schema>dc</dc-schema>
